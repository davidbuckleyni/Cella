@model  Cella.Models.SystemSetup

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_AdminLteLayoutWithoutDashIcons.cshtml";
}

<h1>Edit</h1>

<h4>SystemSetup</h4>

<div class="col-12 col-sm-12">
    <div class="card card-primary card-tabs">
        <div class="card-header p-0 pt-1">
            <ul class="nav nav-tabs" id="custom-tabs-one-tab" role="tablist">

                <li class="nav-item">
                    <a class="nav-link" id="custom-tabs-one-settings-tab" data-toggle="pill" href="#custom-tabs-one-settings" role="tab" aria-controls="custom-tabs-one-image" aria-selected="false"> Settings</a>
                </li>

            </ul>
        </div>

        <div class="card-body">
            <div class="tab-content" id="custom-tabs-one-tabContent">
                <div class="tab-pane fade show active" id="custom-tabs-one-settings" role="tabpanel" aria-labelledby="custom-tabs-one-settings-tab">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="Id" />
                        <div class="form-group">
                            <label asp-for="Language" class="control-label"></label>
                            <input asp-for="Language" class="form-control" />
                            <span asp-validation-for="Language" class="text-danger"></span>
                        </div>
                   
                        <div class="form-group">
                            <label asp-for="SendGridApiKey" class="control-label"></label>
                            <input asp-for="SendGridApiKey" class="form-control" />
                            <span asp-validation-for="SendGridApiKey" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="SendGridUserName" class="control-label"></label>
                            <input asp-for="SendGridUserName" class="form-control" />
                            <span asp-validation-for="SendGridUserName" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="SendGridPassword" class="control-label"></label>
                            <input asp-for="SendGridPassword" class="form-control" />
                            <span asp-validation-for="SendGridPassword" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="TwilloUrl" class="control-label"></label>
                            <input asp-for="TwilloUrl" class="form-control" />
                            <span asp-validation-for="TwilloUrl" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="TwillioUserName" class="control-label"></label>
                            <input asp-for="TwillioUserName" class="form-control" />
                            <span asp-validation-for="TwillioUserName" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="TwillioPassword" class="control-label"></label>
                            <input asp-for="TwillioPassword" class="form-control" />
                            <span asp-validation-for="TwillioPassword" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="UploadFolderPath" class="control-label"></label>
                            <input asp-for="UploadFolderPath" class="form-control" />
                            <span asp-validation-for="UploadFolderPath" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="TeannatId" class="control-label"></label>
                            <input asp-for="TeannatId" class="form-control" />
                            <span asp-validation-for="TeannatId" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="GridItems" class="control-label"></label>
                            <input asp-for="GridItems" class="form-control" />
                            <span asp-validation-for="GridItems" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Version" class="control-label"></label>
                            <input asp-for="Version" class="form-control" />
                            <span asp-validation-for="Version" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Save" class="btn btn-primary" />
                        </div>
                    </form>
                </div>

            </div>
            

        </div>
    </div>
</div>
 