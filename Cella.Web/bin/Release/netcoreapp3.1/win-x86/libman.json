{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatables.net-select@1.3.1",
      "destination": "wwwroot/datatables.net-select/"
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "dropzone@5.7.1",
      "destination": "wwwroot/dropzone/"
    },
    {
      "library": "bootstrap-select@1.13.18",
      "destination": "wwwroot/bootstrap-select/"
    },
    {
      "library": "flag-icon-css@3.5.0",
      "destination": "wwwroot/flag-icon-css/"
    },
    {
      "library": "font-awesome@5.13.1",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/qrcodejs/"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1",
      "destination": "wwwroot/aspnet/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "library": "bootstrap-filestyle@2.1.0",
      "destination": "wwwroot/bootstrap-filestyle/"
    },
    {
      "library": "kendo-ui-core@2014.1.416",
      "destination": "wwwroot/kendo-ui-core/"
    }
  ]
}